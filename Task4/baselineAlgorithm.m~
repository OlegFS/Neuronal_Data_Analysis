% 1- Filter 
xF = filterTraces(x, fs);
% 2- Kernel estimation
% 3- Deconvolution
kern = exppdf(1:100,10);
h = deconvTraces(xF,y,kern);
% 4- Inward rectification - Now inside 
        % ROC curve OR percentage of false positive
        falsePositives(h,y)
        corr(h,y)
% 5- Spike Extraction
s = spikeExtraction(h);
% 6 - Normalization
% 7 - Classification

corrResp = ismember(hLocks,yLocks);
B = logisticReg(s',corrResp)
p = (sigmoid([B'*[0 s(:,1)]'])>=0.5)';

